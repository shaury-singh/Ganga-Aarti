    <section class="book-section">
        <div class="primary-grid pad">
            <h2 class="heading-counter">Book Your Aarti Now!</h2>
            <form action="/book-your-aarti" method="post">
                <div class="book-form-div">
                    <span class="it-cl">Name: </span>
                    <input name="Name" type="text" required class="input" placeholder="Name">
                </div>
                <div class="book-form-div">
                    <span class="it-cl">Email Address: </span>
                    <input name="Email" type="email" required class="input" placeholder="Email-id">
                </div>
                <div class="book-form-div">
                    <span class="it-cl">Mobile Number: </span>
                    <input name="phoneNumber" type="number" required class="input" placeholder="Mobile Number">
                </div>
                <div class="book-form-div">
                    <span class="it-cl">Place: </span>
                    <input name="Place" type="text" required class="input" placeholder="Where Are You From?: Example: Varanasi">
                </div>
                <div class="book-form-div">
                    <span class="it-cl">Tentative Date Of Booking: (mm/dd/yy)</span>
                    <input name="date" type="date" required class="date" placeholder="Date Of Booking (mm/dd/yy)">
                </div>
                <div class="submit-div">
                    <button type="submit">Submit</button>
                </div>
            </form>
        </div>
    </section>
